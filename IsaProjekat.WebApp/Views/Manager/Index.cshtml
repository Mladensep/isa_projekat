@model List<IsaProjekat.WebApp.Models.ManagerModel>
@{
    ViewBag.Title = "Index";
}

<h2>Managers</h2>

<a href="@Url.Action("Register", "Manager")" class="btn btn-primary">Add Manager</a>
<br />
<br />
<table class="table table-bordered table-condensed">
    <thead>
        <tr>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Email</th>
           
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.FirstName</td>
                <td>@item.LastName</td>
                <td>@item.Email</td>
            
            </tr>
        }
    </tbody>
</table>

